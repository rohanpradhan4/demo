class Solution {
    public void reverseString(char[] s) {
        
        int j = s.length-1;
        char c='0';
        
        for(int i=0;i<s.length/2;i++)
        {
          if(i<j)
          {
             c = s[i];
            s[i] = s[j];
            s[j] = c;   
                j--;
        }
           
            
        }
       
        
    }
}
